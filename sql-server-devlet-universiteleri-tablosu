--Devlet üniversiteleri tablosunu oluşturma

CREATE TABLE DevletUniversiteleri
(
    Id INT PRIMARY KEY IDENTITY(1, 1),
    UniversiteAd NVARCHAR(100),
    UniversiteIl NVARCHAR(50)
);


